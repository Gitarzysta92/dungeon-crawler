{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./example/dist",
    "types": [],
  },
  "include": [
    "src/**/*.ts",
    "../utils/src/**/*.ts",
    "../game-logic/src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/dev/*",
  ],
  "ts-node": {
    "esm": false,
    "compilerOptions": {
      "module": "CommonJS",
    },
    "require": ["tsconfig-paths/register"],
    "logError": true,
    
  }
}
